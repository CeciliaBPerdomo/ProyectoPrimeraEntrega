<% if(carrito.length > 0){ %>
    <% for (const item of carrito) {%>
        <p>Tu carrito es el: <strong><%=item.id%></strong>. 
            Su timestamp es: <strong><%=item.timestamp%></strong></p>
        <br>
        <div>
            <h2><strong>
                Sus productos: <br>
            </strong></h2>
        </div>
        
        <div>
            <table class="table table-hover table-dark">
                <thead>
                  <tr>
                    <th scope="col">Imagen</th>
                    <th scope="col">Código</th>
                    <th scope="col">Descripción</th>
                    <th scope="col">Precio</th>
                    <th scope="col"></th>
                  </tr>
                </thead>

                <tr>
                    <td class="table-info"><img src="<%=item.productos.foto%>" width="50px" height="35px"></td>
                    <td class="table-info"><%=item.productos.codigo%></td>
                    <td class="table-info"><%=item.productos.descripcion%></td>
                    <td class="table-info"><%=item.productos.precio%></td>
                    <td class="table-info">
                        <button class="btn btn-outline-primary">Modificar</button>
                        <button class="btn btn-outline-danger">Eliminar</button>
                    </td>
                </tr>
            </table>
        </div>
    <%};%>
<%};%>